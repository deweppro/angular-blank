{
  "name": "application",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "app:start": "ng serve",
    "app:build": "ng build --base-href ./ --subresource-integrity",
    "app:watch": "yarn run app:build -- --watch --configuration development",
    "app:lint": "ng lint lib --fix",
    "app:icon": "pwa-asset-generator src/logo.svg src/assets/icons -m src/manifest.webmanifest -v /assets/icons -f -c -w -s false -o false -p 2%",
    "electron:start": "electron-forge start --inspect-electron --enable-logging",
    "electron:package": "electron-forge package",
    "electron:make": "electron-forge make --targets @electron-forge/maker-deb",
    "start": "yarn run app:build && yarn run electron:start"
  },
  "main": "./dist/application/electron.js",
  "engines": {
    "node": "^16.13.0 || >=18.10.0",
    "yarn": ">=1.21.1 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/pwa": "0.5.3",
    "@angular/router": "^14.0.0",
    "@angular/service-worker": "^14.0.0",
    "@uxwb/components": "^14.0.0",
    "@uxwb/services": "^14.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "^23.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.0.0",
    "pwa-asset-generator": "^6.2.1",
    "typescript": "~4.7.2"
  }
}
